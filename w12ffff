#!/bin/sh
clear
tput setaf 6; echo "              .               "
tput setaf 6; echo "            . i .             "
echo -e "$(tput setaf 6)           . $(tput setaf 58)\|/ $(tput setaf 6).            "
echo -e "$(tput setaf 6)          . $(tput setaf 58)\\$(tput setaf 6) $(tput setaf 58)V$(tput setaf 6) $(tput setaf 58)/$(tput setaf 6) .            "
echo -e "$(tput setaf 58)           \\ _$(tput setaf 58)Y_ $(tput setaf 588)$(tput setaf 58)$(tput setaf 58)/             "
echo -e "$(tput setaf 58)           \\$(tput setaf 58)\\.|./$(tput setaf 58)/             "
echo -e "$(tput setaf 94)          .d\\$(tput setaf 58)TMT$(tput setaf 94)/b.            "#80FF00
echo -e "$(tput setaf 94)         .AAA\\$(tput setaf 58)W$(tput setaf 94)/AAA.           "
echo -e "$(tput setaf 250)         [    $(tput setaf 94)Y$(tput setaf 250)    ]          "
echo -e "$(tput setaf 250)         ( 0  $(tput setaf 94)x$(tput setaf 250)  0 )          "
echo -e "$(tput setaf 250)          \\$(tput setaf 250)__$(tput setaf 94)/A$(tput setaf 94)\\$(tput setaf 250)__$(tput setaf 250)/           "
echo -e "$(tput setaf 94)           V$(tput setaf 208)/AWA\\$(tput setaf 94)V            "
echo -e "$(tput setaf 94)           .$(tput setaf 208)Y$(tput setaf 1).$(tput setaf 208)T$(tput setaf 1).$(tput setaf 208)Y$(tput setaf 94).            "
echo -e "$(tput setaf 94)          .A $(tput setaf 208)VWV $(tput setaf 94)A.          "
echo -e "$(tput setaf 94)         .//$(tput setaf 58)A $(tput setaf 208)V$(tput setaf 58) A$(tput setaf 94)\\\\\.          "
echo -e "$(tput setaf 94)       .///$(tput setaf 58).M$(tput setaf 247):.:$(tput setaf 58)M.$(tput setaf 94)\\\\\\\\\.       "
echo -e "$(tput setaf 94)     .////$(tput setaf 58).M.$(tput setaf 247),^,$(tput setaf 58).M.$(tput setaf 94)\\\\\\\\\\\\\\.      "
echo -e "$(tput setaf 94)    /////$(tput setaf 58).W.M$(tput setaf 247),^,$(tput setaf 58)M.W.$(tput setaf 94)\\\\\\\\\\\\\\\\\\\\     "
echo -e "$(tput setaf 94)   /////$(tput setaf 58).W.M.$(tput setaf 247),^,$(tput setaf 58).M.W.$(tput setaf 94)\\\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 94)   /////$(tput setaf 58)M.W.M$(tput setaf 247),^.$(tput setaf 58)M.W.M$(tput setaf 94)\\\\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 94)   '////$(tput setaf 58).W.M.$(tput setaf 247),^,$(tput setaf 58).M.W.$(tput setaf 94)\\\\\\\\\\\\\\\'      "
echo -e "$(tput setaf 94)    '//.$(tput setaf 58)M.W.M$(tput setaf 247),^,$(tput setaf 58)M.W.M$(tput setaf 94).\\\\\\\\'       "
echo -e "$(tput setaf 58)      'W.M$(tput setaf 58).W.$(tput setaf 247),^,$(tput setaf 58).W.$(tput setaf 58)M.W'       "
echo -e "$(tput setaf 58)        'TW$(tput setaf 58)MW$(tput setaf 247)M^M$(tput setaf 58)WMWT'         "
echo -e "$(tput setaf 220)          1i$(tput setaf 58)jjAjj$(tput setaf 220)i1           "
echo -e "$(tput setaf 220)          1i     i1           "
echo -e "$(tput setaf 220)          1i     i1           "
echo -e "$(tput setaf 208)         MMMM   MMMM           "
echo -e "$(tput setaf 208)        M|MM|M M|MM|M          "
