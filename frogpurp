#!/bin/sh
echo ' '
echo -e $(tput setaf 57)"                                                                                                  "
echo -e $(tput setaf 57)"                                               .xOOx.                                             "
echo -e $(tput setaf 220)"                 .. $(tput setaf 57)                        .dXXXXXXXXb.                        $(tput setaf 220) ..               "
echo -e $(tput setaf 220)"               dXXXXb                     $(tput setaf 202).$(tput setaf 57)dXXXXXXXXXXXXb$(tput setaf 202).                     $(tput setaf 220)dXXXXb             "
echo -e $(tput setaf 220)"               *XXXX    ..              $(tput setaf 202).0.$(tput setaf 57)XXXXXXXXXXXXXX$(tput setaf 202).0.              $(tput setaf 220)..    XXXX*             "
echo -e $(tput setaf 220)"           ..    *X.   $(tput setaf 220)iXXX.           $(tput setaf 202).00.$(tput setaf 57)XXXXX$(tput setaf 196)....$(tput setaf 57)XXXXX$(tput setaf 202).00.           $(tput setaf 220).XXXi   .X*    ..         "
echo -e $(tput setaf 220)"         dXXX   $(tput setaf 57) *X.   $(tput setaf 220)XXX*           $(tput setaf 202).000.$(tput setaf 57)XXXX$(tput setaf 196)......$(tput setaf 57)XXXX$(tput setaf 202).000.           $(tput setaf 220)*XXX   $(tput setaf 57).X*    $(tput setaf 220)XXXb       "
echo -e $(tput setaf 220)"          XXXX..$(tput setaf 57) .XX. .X'             $(tput setaf 202).000.$(tput setaf 57)XXXXXX$(tput setaf 196)..$(tput setaf 57)XXXXXX$(tput setaf 202).000.             $(tput setaf 57)'X. .XX. $(tput setaf 220)..XXXX        "
echo -e $(tput setaf 57)"             ^*XXXXXXXXX               $(tput setaf 202).0.$(tput setaf 57)XXXX$(tput setaf 190)II$(tput setaf 57)XXXX$(tput setaf 190)II$(tput setaf 57)XXXX$(tput setaf 202).0.               $(tput setaf 57)XXXXXXXXX*^           "
echo -e $(tput setaf 57)"                 XXXXXXX. $(tput setaf 220)vXX.          $(tput setaf 57).XXXX$(tput setaf 190)IIII$(tput setaf 57)XX$(tput setaf 190)IIII$(tput setaf 57)XXXX.          $(tput setaf 220).XXv $(tput setaf 57).XXXXXXX               "
echo -e $(tput setaf 57)"                   qXXXXxx$(tput setaf 220)XXXX          $(tput setaf 57).XX$(tput setaf 196)..$(tput setaf 57)XXX$(tput setaf 190)IIII$(tput setaf 57)XXX$(tput setaf 196)..$(tput setaf 57)XX.           $(tput setaf 220)XXXX$(tput setaf 57)xxXXXXp                "
echo -e $(tput setaf 57)"                    *xxx   $(tput setaf 220)^*           $(tput setaf 57).XXX$(tput setaf 196)..$(tput setaf 57)XX$(tput setaf 190)IIII$(tput setaf 57)XX$(tput setaf 196)..$(tput setaf 57)XXX.           $(tput setaf 220) *^   $(tput setaf 57)XXX*                 "
echo -e $(tput setaf 57)"                     *xx.              .xxxxx$(tput setaf 196)..$(tput setaf 57)xx$(tput setaf 190)II$(tput setaf 57)xx$(tput setaf 196)..$(tput setaf 57)xxxxx.              .xx*                   "
echo -e $(tput setaf 57)"                      *xx.          .xxxxxxxxxx$(tput setaf 196)......$(tput setaf 57)xxxxxxxxxx.          .xx*                    "
echo -e $(tput setaf 57)"                       *xxx     .xxxxxxxxxxxxxxxx$(tput setaf 196)..$(tput setaf 57)xxxxxxxxxxxxxxxx.     xxx*                     "
echo -e $(tput setaf 57)"                        qxxx..xxxxxxxxxxxxxx$(tput setaf 196).$(tput setaf 57)xxxx$(tput setaf 196)..$(tput setaf 57)xxxx$(tput setaf 196).$(tput setaf 57)xxxxxxxxxxxxxx..xxxp                      "
echo -e $(tput setaf 57)"                         qxxxxxxxxp  qxxxxx$(tput setaf 196).$(tput setaf 190)I$(tput setaf 196).$(tput setaf 57)xxx$(tput setaf 196)..$(tput setaf 57)xxx$(tput setaf 196).$(tput setaf 190)I$(tput setaf 196).$(tput setaf 57)xxxxxp  qxxxxxxxxp                       "
echo -e $(tput setaf 57)"                           *xxxp*    xxxxx$(tput setaf 196).$(tput setaf 190)III$(tput setaf 196).$(tput setaf 57)xx$(tput setaf 196)..$(tput setaf 57)xx$(tput setaf 196).$(tput setaf 190)III$(tput setaf 196).$(tput setaf 57)xxxxx    *qxxx*                         "
echo -e $(tput setaf 57)"                                   .x$(tput setaf 196)..$(tput setaf 57)xxxx$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)xx$(tput setaf 196)..$(tput setaf 57)xx$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)xxxx$(tput setaf 196)..$(tput setaf 57)x.                                 "
echo -e $(tput setaf 57)"                                  .xx$(tput setaf 196)...$(tput setaf 57)xxxx$(tput setaf 196)..$(tput setaf 57)xxx$(tput setaf 196)..$(tput setaf 57)xxx$(tput setaf 196)..$(tput setaf 57)xxxx$(tput setaf 196)...$(tput setaf 57)xx.                                "
echo -e $(tput setaf 57)"                                  XXXX$(tput setaf 196).$(tput setaf 57)XXXXXXXXXx$(tput setaf 196)..$(tput setaf 57)xXXXXXXXXX$(tput setaf 196).$(tput setaf 57)XXXX                                "
echo -e $(tput setaf 57)"                                 .XXXXXx$(tput setaf 196).$(tput setaf 57)xXXXXXx$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)xXXXXXx$(tput setaf 196).$(tput setaf 57)xXXXXX.                               "
echo -e $(tput setaf 57)"                                 .XXXx$(tput setaf 196).$(tput setaf 190)iIi$(tput setaf 196).$(tput setaf 57)xXXx$(tput setaf 196).$(tput setaf 190)iIIi$(tput setaf 196).$(tput setaf 57)xXXx$(tput setaf 196).$(tput setaf 190)iIi$(tput setaf 196).$(tput setaf 57)xXXX.                               "
echo -e $(tput setaf 57)"                                 .xXx$(tput setaf 196).$(tput setaf 190)IiIiI$(tput setaf 196).$(tput setaf 57)xXx$(tput setaf 196).$(tput setaf 190)iIIi$(tput setaf 196).$(tput setaf 57)xXx$(tput setaf 196).$(tput setaf 190)IiIiI$(tput setaf 196).$(tput setaf 57)xXx.                               "
echo -e $(tput setaf 220)"         .dxx.   dXX. $(tput setaf 57)dXXx..      .xXx$(tput setaf 196).$(tput setaf 190)iIi$(tput setaf 196).$(tput setaf 57)xXXXx$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)xXXXx$(tput setaf 196).$(tput setaf 190)iIi$(tput setaf 196).$(tput setaf 57)xXX.      ..xXXb $(tput setaf 220).XXb   .xxb.       "
echo -e $(tput setaf 220)"         XXXCX   XXXp $(tput setaf 57)*XXXXXXXx.   xxXxx$(tput setaf 196).$(tput setaf 57)xxXXXXXx$(tput setaf 196)..$(tput setaf 57)xXXXXXXx$(tput setaf 196).$(tput setaf 57)xxXxx   .xXXXXXXX* $(tput setaf 220)qXXX   XXXXX       "
echo -e $(tput setaf 220)"          XXXX.   XX   $(tput setaf 57)XXXXXX$(tput setaf 196)..$(tput setaf 57)XXXx.XXXxxxXXXXXXX$(tput setaf 196)..$(tput setaf 57)XXXXXXXXXXXXX.xXXX$(tput setaf 196)..$(tput setaf 57)XXXXXX   $(tput setaf 220)XX   .XXXX        "
echo -e $(tput setaf 57)"             *XXXx.Xx   XXXXX$(tput setaf 196)...$(tput setaf 57)XXXXXXXXX$(tput setaf 196)..$(tput setaf 57)XXXXXx$(tput setaf 196)..$(tput setaf 57)xXXXXX$(tput setaf 196)..$(tput setaf 57)XXXXXXXXX$(tput setaf 196)...$(tput setaf 57)XXXXX   xX.xXXX*           "
echo -e $(tput setaf 220)"     .XXX.     $(tput setaf 57).XXXXXb   XXXXXXXXXXXXXXX$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)XXXx$(tput setaf 196)....$(tput setaf 57)xXXX$(tput setaf 196).$(tput setaf 190)II$(tput setaf 196).$(tput setaf 57)XXXXXXXXXXXXXXX   dXXXXX.     $(tput setaf 220).XXX.   "
echo -e $(tput setaf 220)"     xXXXX$(tput setaf 57)XXXXXXXXXXXXXb  XXXXXX  ^XXXXXX$(tput setaf 196)..$(tput setaf 57)XXXx$(tput setaf 196).$(tput setaf 190)iIIi$(tput setaf 196).$(tput setaf 57)xXXX$(tput setaf 196)..$(tput setaf 57)XXXXXX^  XXXXXX  dXXXXXXXXXXXXX$(tput setaf 220)XXXXx   "
echo -e $(tput setaf 220)"      *XX*          $(tput setaf 57)*XXXXx.XXXXX.    ^XXXXXXXx$(tput setaf 196).$(tput setaf 190)IiIIiI$(tput setaf 196).$(tput setaf 57)xXXXXXXX^    .XXXXX.xXXXX*          $(tput setaf 220)*XX*    "
echo -e $(tput setaf 57)"                        XXXXXXXXX.      ^XXXx$(tput setaf 196).$(tput setaf 190)iIiIIiIi$(tput setaf 196).$(tput setaf 57)xXXX^      .XXXXXXXXX                      "
echo -e $(tput setaf 57)"                          XXXXXXXb        *x$(tput setaf 196).$(tput setaf 190)IiIiIIiIiI$(tput setaf 196).$(tput setaf 57)x*        dXXXXXXX                        "
echo -e $(tput setaf 57)"                            XXXXXXb          $(tput setaf 196)*..$(tput setaf 190)iIIi$(tput setaf 196)..*          $(tput setaf 57)dXXXXXX                          "
echo -e $(tput setaf 57)"                              XXXXX.           $(tput setaf 196)^*..*^           $(tput setaf 57).XXXXX                            "
echo -e $(tput setaf 57)"                                XXX                              xxx                              "
echo ' '
