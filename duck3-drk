#!/bin/sh
clear
tput setaf 118; echo "              .               "
tput setaf 118; echo "            . : .             "
echo -e "$(tput setaf 118)           . $(tput setaf 58)\|/ $(tput setaf 118).            "
echo -e "$(tput setaf 118)          . $(tput setaf 58)\\$(tput setaf 118) $(tput setaf 58)V$(tput setaf 118) $(tput setaf 58)/$(tput setaf 118) .            "
echo -e "$(tput setaf 58)           \\ _$(tput setaf 58)Y_ $(tput setaf 588)$(tput setaf 58)$(tput setaf 58)/             "
echo -e "$(tput setaf 136)           .$(tput setaf 58)\\:W:/$(tput setaf 136).             "
echo -e "$(tput setaf 136)          .T\\$(tput setaf 58)WMW$(tput setaf 136)/T.            "
echo -e "$(tput setaf 136)         .AAA\\$(tput setaf 58)W$(tput setaf 136)/AAA.           "
echo -e "$(tput setaf 250)         [    $(tput setaf 136)Y$(tput setaf 250)    ]          "
echo -e "$(tput setaf 250)         ( 0  $(tput setaf 136)x$(tput setaf 250)  0 )          "
echo -e "$(tput setaf 250)          \\ $(tput setaf 136) /A$(tput setaf 136)\ $(tput setaf 250) /           "
echo -e "$(tput setaf 136)           \\$(tput setaf 208)/AWA\\$(tput setaf 136)/            "
echo -e "$(tput setaf 136)           .$(tput setaf 208)Y$(tput setaf 1).$(tput setaf 208)T$(tput setaf 1).$(tput setaf 208)Y$(tput setaf 136).            "
echo -e "$(tput setaf 136)          .A $(tput setaf 208)VWV $(tput setaf 136)A.          "
echo -e "$(tput setaf 136)         .//$(tput setaf 58)A $(tput setaf 208)V$(tput setaf 58) A$(tput setaf 136)\\\\\.          "
echo -e "$(tput setaf 136)       .///$(tput setaf 58).M$(tput setaf 247),.,$(tput setaf 58)M.$(tput setaf 136)\\\\\\\\\.       "
echo -e "$(tput setaf 136)     .////$(tput setaf 58).M.$(tput setaf 247),^,$(tput setaf 58).M.$(tput setaf 136)\\\\\\\\\\\\\\.      "
echo -e "$(tput setaf 136)    /////$(tput setaf 58).W.M$(tput setaf 247),^,$(tput setaf 58)M.W.$(tput setaf 136)\\\\\\\\\\\\\\\\\\\\     "
echo -e "$(tput setaf 136)   /////$(tput setaf 58).W.M.,^,.M.W.$(tput setaf 136)\\\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 136)   /////$(tput setaf 58)M.W.M,^.M.W.M$(tput setaf 136)\\\\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 136)   '////$(tput setaf 58).W.M.,^,.M.W.$(tput setaf 136)\\\\\\\\\\\\\\\'      "
echo -e "$(tput setaf 136)    '//.$(tput setaf 58)M.W.M,^,M.W.M$(tput setaf 136).\\\\\\\\'       "
echo -e "$(tput setaf 136)      'W.M$(tput setaf 58).W.,^,.W.$(tput setaf 136)M.W'       "
echo -e "$(tput setaf 136)        'TWMWM^MWMWT'         "
echo -e "$(tput setaf 136)          11jjAjji1           "
echo -e "$(tput setaf 190)          i1     i1           "
echo -e "$(tput setaf 190)          i1     i1           "
echo -e "$(tput setaf 136)         MMMM   MMMM           "
echo -e "$(tput setaf 136)        M|MM|M M|MM|M          "
