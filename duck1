#!/bin/sh
clear
tput setaf 118; echo "              .               "
tput setaf 118; echo "            . i .             "
echo -e "$(tput setaf 118)           . $(tput setaf 58)\|/ $(tput setaf 118).            "
echo -e "$(tput setaf 118)          . $(tput setaf 58)\\$(tput setaf 118) $(tput setaf 58)V$(tput setaf 118) $(tput setaf 58)/$(tput setaf 118) .            "
echo -e "$(tput setaf 58)           \\_$(tput setaf 58)\Y/_$(tput setaf 58 8)$(tput setaf 58)$(tput setaf 58)/             "
echo -e "$(tput setaf 58)           \\$(tput setaf 58)\\.|./$(tput setaf 58)/             "
echo -e "$(tput setaf 94)          .d\\$(tput setaf 58)TMT$(tput setaf 94)/b.            "
echo -e "$(tput setaf 94)         .AAA\\$(tput setaf 58)W$(tput setaf 94)/AAA.           "
echo -e "$(tput setaf 4)         [    $(tput setaf 94)Y$(tput setaf 4)    ]          "
echo -e "$(tput setaf 4)         ( 0  $(tput setaf 94)x$(tput setaf 4)  0 )          "
echo -e "$(tput setaf 4)          \\$(tput setaf 4)__$(tput setaf 94)/A$(tput setaf 94)\\$(tput setaf 4)__$(tput setaf 4)/           "
echo -e "$(tput setaf 94)           V$(tput setaf 208)/AWA\\$(tput setaf 94)V            "
echo -e "$(tput setaf 94)           .$(tput setaf 208)Y$(tput setaf 1).$(tput setaf 208)T$(tput setaf 1).$(tput setaf 208)Y$(tput setaf 94).            "
echo -e "$(tput setaf 94)          .A $(tput setaf 208)VWV $(tput setaf 94)A.          "
echo -e "$(tput setaf 94)         ./$(tput setaf 166)/$(tput setaf 58)A $(tput setaf 208)V$(tput setaf 58) A$(tput setaf 166)\\$(tput setaf 94)\\\\\.          "
echo -e "$(tput setaf 94)       .//$(tput setaf 166)/.$(tput setaf 58)M$(tput setaf 247):.:$(tput setaf 58)M$(tput setaf 166).\\$(tput setaf 94)\\\\\\\\.       "
echo -e "$(tput setaf 94)     .///$(tput setaf 166)/.$(tput setaf 58)M$(tput setaf 247).,$(tput setaf 256)^$(tput setaf 247),.$(tput setaf 58)M$(tput setaf 166).\\$(tput setaf 94)\\\\\\\\\\\\.      "
echo -e "$(tput setaf 94)    ////$(tput setaf 166)/.$(tput setaf 58)W.M$(tput setaf 247),$(tput setaf 256)^$(tput setaf 247),$(tput setaf 58)M.W$(tput setaf 166).\\$(tput setaf 94)\\\\\\\\\\\\\\\\     "
echo -e "$(tput setaf 94)   ////$(tput setaf 166)/.$(tput setaf 58)W.M$(tput setaf 247).,$(tput setaf 256)^$(tput setaf 247),.$(tput setaf 58)M.W$(tput setaf 166).\\$(tput setaf 94)\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 94)   ////$(tput setaf 166)/$(tput setaf 58)M.W.M$(tput setaf 247),$(tput setaf 256)^$(tput setaf 247).$(tput setaf 58)M.W.M$(tput setaf 166)\\$(tput setaf 94)\\\\\\\\\\\\\\\\    "
echo -e "$(tput setaf 94)   '///$(tput setaf 166)/.$(tput setaf 58)W.M$(tput setaf 247).,$(tput setaf 256)^$(tput setaf 247),.$(tput setaf 58)M.W$(tput setaf 166).\\$(tput setaf 94)\\\\\\\\\\\'      "
echo -e "$(tput setaf 94)    '/$(tput setaf 166)/.$(tput setaf 58)M.W.M$(tput setaf 247),$(tput setaf 256)^$(tput setaf 247),$(tput setaf 58)M.W.M$(tput setaf 166).\\$(tput setaf 94)\\'       "
echo -e "$(tput setaf 58)      'W.M$(tput setaf 247).W.,$(tput setaf 256)^$(tput setaf 247),.W.$(tput setaf 58)M.W'       "
echo -e "$(tput setaf 58)        'TW$(tput setaf 247)MWM^MWM$(tput setaf 58)WT'         "
echo -e "$(tput setaf 220)          1i$(tput setaf 58)jjAjj$(tput setaf 220)i1           "
echo -e "$(tput setaf 220)          1i     i1           "
echo -e "$(tput setaf 220)          1i     i1           "
echo -e "$(tput setaf 208)         MMMM   MMMM           "
echo -e "$(tput setaf 208)        M|MM|M M|MM|M          "
